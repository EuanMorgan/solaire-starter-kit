    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-seed": "^0.3.1",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "playwright": "^1.57.0",
    "storybook": "^10.2.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
