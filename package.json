    "postgres": "^3.4.8",
    "posthog-js": "^1.334.1",
    "posthog-node": "^5.24.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "resend": "^6.8.0",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
